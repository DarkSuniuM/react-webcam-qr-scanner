{
  "name": "react-webcam-qr-scanner",
  "version": "1.0.0",
  "description": "A humble implementation of a real-time QR-code scanner using WebRTC",
  "main": "./dist/index.js",
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "jsqr": "latest",
    "prop-types": "latest",
    "worker-loader": "latest"
  },
  "peerDependencies": {
    "react": "latest"
  },
  "devDependencies": {
    "@babel/cli": "latest",
    "@babel/core": "latest",
    "@babel/plugin-proposal-class-properties": "latest",
    "@babel/preset-env": "latest",
    "@babel/preset-react": "latest",
    "rimraf": "latest"
  },
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run clean && npm run transpile",
    "clean": "rimraf dist",
    "transpile": "babel src -d dist --copy-files"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simpleman383/react-webcam-qr-scanner.git"
  },
  "author": "simpleman383",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/simpleman383/react-webcam-qr-scanner/issues"
  },
  "homepage": "https://github.com/simpleman383/react-webcam-qr-scanner#readme"
}
